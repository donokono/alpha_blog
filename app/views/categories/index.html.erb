<div class="row">
  <div class="col-xs-12">
    <h1>All Categories</h1>
    <%= will_paginate %>
    <% @categories.each do |category| %>
    <div class="row">
      <div class="col-xs-12">
        <div class="well">
          <h3>
            <%= link_to "#{category.name}", category_path(category) %>
          </h3>
        </div>
      </div>
    </div>
    <% end %>
    <%= will_paginate %>
  </div>
</div>